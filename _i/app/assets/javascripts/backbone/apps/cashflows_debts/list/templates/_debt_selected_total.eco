<% balance = @balance() %>
<% moneys = @moneys() %>

<td colspan="3">
    Итого по выделенным долгам:
</td>

<td class="sum">
    <% for own key, idMoney of moneys: %>
    <div class="row">
        <div class="col-sm-12 text-right">
            <% if not _.isUndefined(balance[idMoney]['debt']): %>
            <%- @numberToMoney(balance[idMoney]['debt'], idMoney) %>
            <% else: %>
            <%- '&nbsp;' %>
            <% end %>
        </div>
    </div>
    <% end %>
</td>
<td class="symbol">
    <% for own key, idMoney of moneys: %>
    <div class="row">
        <div class="col-sm-12">
            <% if not _.isUndefined(balance[idMoney]['debt']): %>
            <%- CashFlow.entities.moneys.get(idMoney).get('symbol') %>
            <% else: %>
            <%- '&nbsp;' %>
            <% end %>
        </div>
    </div>
    <% end %>
</td>

<td class="sum">
    <% for own key, idMoney of moneys: %>
    <div class="row">
